//output:index.html
doctype html
html
  head
    meta(charset='utf8')
    link(rel='stylesheet' href='https://getuikit.com/assets/uikit/dist/css/uikit.css')
    meta(name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no")
    link(rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css')
    title SM Hacks LIVE
  body.uk-background-muted
    nav.uk-navbar-container(uk-navbar=true)
      .uk-navbar-left
        a.uk-navbar-item.uk-logo(href='#')
          |SM Hacks LIVE&nbsp;
          i.fa.fa-video-camera
      .uk-navbar-right
        ul.uk-navbar-nav
          li: a(href='#nav' uk-toggle=true)
            i.fa.fa-bars.fa-2x

    .uk-container(style='margin-top:20px')
      .uk-grid-medium(uk-grid=true)
        div(class='uk-width-expand@m'): .uk-card.uk-card-default.uk-card-body
          h1.uk-text-center.uk-text-warning Hacking has ended.
      .uk-grid-medium(uk-grid=true)
        div(class='uk-width-expand@m'): .uk-card.uk-card-default.uk-card-body
          h1 Hacking's End
          p At 11:00, Devpost submissions will open for hackers. Make sure you submit your project in order to qualify for awards!
        div(class='uk-width-auto@m')
          .uk-card.uk-card-default.uk-card-body
            h1 Next events
            p <code>12:00pm</code> - Hacking ends, lunch
            p <code>12:30pm</code> - Demos, Judging
            p <code>01:45pm</code> - Closing Ceremony
          br
          .uk-card.uk-card-default.uk-card-body <a class="twitter-timeline" data-height="400" data-theme="light" data-link-color="#E95F28" href="https://twitter.com/sanmateohacks">Tweets by sanmateohacks</a> <script async src="//platform.twitter.com/widgets.js" charset="utf-8"></script>
    
    #nav(uk-offcanvas='overlay:true;flip:true;mode:reveal')
      .uk-offcanvas-bar
        p: a(href='https://smhacks.devpost.com/' target='_blank')
          i.fa.fa-code.fa-fw
          |&nbsp;Devpost
        p: a(href='https://drive.google.com/file/d/0B6tVJ5ml7Bt4NjBlbXNXcEZsUE0/view?usp=sharing' target='_blank')
          i.fa.fa-map.fa-fw
          |&nbsp;Floor plan
        p: a(href='http://waiver.smhacks.com' target='_blank')
          i.fa.fa-file-text.fa-fw
          |&nbsp;Waiver
        hr
        p: a(href='https://itunes.apple.com/us/app/sm-hacks/id1193112362' target='_blank')
          i.fa.fa-apple.fa-fw
          |&nbsp;iOS App
        p: a(href='https://facebook.com/sanmateohacks' target='_blank')
          i.fa.fa-facebook.fa-fw
          |&nbsp;Facebook
        p: a(href='https://twitter.com/sanmateohacks' target='_blank')
          i.fa.fa-twitter.fa-fw
          |&nbsp;Twitter
        p: a(href='http://slack.smhacks.com' target='_blank')
          i.fa.fa-slack.fa-fw
          |&nbsp;Slack
        
    script(src='https://cdnjs.cloudflare.com/ajax/libs/jquery/3.1.1/jquery.min.js')
    script(src='https://getuikit.com/assets/uikit/dist/js/uikit.js')